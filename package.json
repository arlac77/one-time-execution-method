{
  "name": "one-time-execution-method",
  "version": "0.0.0-semantic-release",
  "sideEffects": false,
  "type": "module",
  "main": "./src/one-time-execution-method.mjs",
  "exports": {
    ".": "./src/one-time-execution-method.mjs"
  },
  "description": "define a method that will be executed only once",
  "keywords": [
    "async",
    "once",
    "one",
    "one-time",
    "single"
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/one-time-execution-method.mjs --section=API && documentation readme ./src/one-time-execution-method.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint src/one-time-execution-method.mjs && documentation lint ./src/one-time-execution-method.mjs"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.5",
    "documentation": "^13.1.0",
    "semantic-release": "^17.2.1"
  },
  "engines": {
    "node": ">=12.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/one-time-execution-method.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/one-time-execution-method/issues"
  },
  "homepage": "https://github.com/arlac77/one-time-execution-method#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github"
    ]
  }
}
